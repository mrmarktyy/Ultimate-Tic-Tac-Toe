{
  "name": "ultimate",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "async": "^2.1.2",
    "connect-mongo": "^1.3.2",
    "dotenv": "^2.0.0",
    "jade": "^1.11.0",
    "keystone": "git@github.com:ratecity/keystone.git",
    "keystone-storage-adapter-s3": "^1.1.0",
    "lodash": "^4.13.1",
    "mongoose-unique-validator": "^1.0.2",
    "node-fetch": "^1.6.0",
    "node-sass": "^3.3.2",
    "node-sass-middleware": "^0.10.0",
    "node-uuid": "^1.4.7",
    "randomstring": "^1.1.5",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.0"
  },
  "devDependencies": {
    "eslint": "^3.10.2",
    "eslint-config-keystone": "^3.0.0",
    "eslint-plugin-react": "^6.7.1",
    "gulp": "^3.7.0",
    "gulp-sass": "^2.0.4",
    "gulp-shell": "^0.5.0",
    "gulp-watch": "^4.3.5",
    "pre-commit": "1.1.3"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "node keystone.js",
    "lint:changed": "LIST=`git diff-index --name-only HEAD | grep '.jsx\\{0,1\\}'$`; if [ \"$LIST\" ]; then eslint $LIST; fi",
    "lint:changed:fix": "LIST=`git diff-index --name-only HEAD | grep '.jsx\\{0,1\\}'$`; if [ \"$LIST\" ]; then eslint $LIST --fix; fi",
    "lint:changed-PR": "LIST=`git diff-index --name-only HEAD^ | grep '.jsx\\{0,1\\}'$`; if [ \"$LIST\" ]; then eslint $LIST; fi"
  },
  "precommit": {
    "run": [
      "lint:changed"
    ]
  }
}
